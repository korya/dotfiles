# vim: set filetype=sh :

if [[ -n "${BASH_VERSION}" ]]; then
  complete -o filenames -F _longopt grepx grepm grepc
  complete -c rwhich
fi

### _ssh_umount()
### {
###     local cur
### 
###     COMPREPLY=()
###     cur=${COMP_WORDS[COMP_CWORD]}
### 
###     COMPREPLY=( $(compgen -W "$(ls $HOME/mnt)" -- $cur) )
### }
### complete -o dirnames -F _ssh_umount ssh_umount
